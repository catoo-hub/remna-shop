###############################################################
#                REMNAWAVE (замена 3x-ui)                     #
###############################################################
# Полный базовый URL панели Remnawave
REMNA_BASE_URL=https://new.aepstore.su

# Bearer токен (создается в панели Remnawave в разделе API Tokens)
REMNA_API_TOKEN=ВАШ_API_ТОКЕН

# Session cookie для доступа к API (если требуется)
REMNA_COOKIE=ВАШ_COOKIE

# Укажите либо тег inbound (tag), либо его UUID (одно из):
REMNA_INBOUND_TAG=VLESS SWE
# REMNA_INBOUND_UUID=f82631ed-3ea7-415c-a4f1-f5379e0d3912

# UUID внутреннего отряда (squad) для добавления пользователей
REMNA_SQUAD_UUID=ВАШ_SQUAD_UUID

# Количество дней по умолчанию при создании (если не передано явно)
REMNA_DEFAULT_DAYS=30

# Переопределение SNI (если нужно отличное от serverNames[0])
REMNA_SERVER_SNI=teamdocs.su

# Fingerprint (uTLS) для клиента (chrome / safari / edge ...)
REMNA_FP=chrome

# Flow для VLESS Reality (по умолчанию xtls-rprx-vision)
REMNA_FLOW=xtls-rprx-vision

# Публичный ключ Reality (если панель не возвращает его напрямую)
REMNA_PUBLIC_KEY=PUBLIC_KEY_PLACEHOLDER

# Лимит трафика в гигабайтах на период стратегии (по умолчанию 500 ГБ)
REMNA_TRAFFIC_LIMIT_GB=500

# Стратегия лимита (MONTH / INFINITE / TOTAL) — MONTH = авто-сброс панелью ежемесячно
REMNA_TRAFFIC_STRATEGY=MONTH

# Имя сервера (для отображения пользователю)
SERVER_NAME="Sweden"

# ===============================================================
#                  НАСТРОЙКИ TELEGRAM-БОТА
# ===============================================================
# Настройки для интеграции с Telegram.

# API-токен вашего Telegram-бота.
# Чтобы его получить, создайте нового бота в Telegram через официального @BotFather.
TELEGRAM_BOT_TOKEN=1234567890:ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890

# Имя пользователя (username) вашего Telegram-бота без символа "@".
# Вы также получаете его при создании бота в @BotFather.
# Пример: MySuperVPNBot
TELEGRAM_BOT_USERNAME=username

# Ваш личный числовой Telegram ID для получения прав администратора в боте.
# Чтобы узнать свой ID, просто напишите любому боту для получения информации о пользователе, например, @userinfobot.
ADMIN_TELEGRAM_ID=1234567890

# ===============================================================
#                 НАСТРОЙКИ TELEGRAM STARS
# ===============================================================
# Настройки для приема платежей через Telegram Stars.

# Включить/выключить оплату звездами (true/false)
STARS_ENABLED=true

# Курс конвертации: сколько звезд за 1 рубль (например, 1.5 = 1.5 звезды за рубль)
STARS_RATE=1.5

# ===============================================================
#                     НАСТРОЙКИ YOOKASSA
# ===============================================================
# Настройки для приема платежей через YooKassa.

# ID вашего магазина в YooKassa.
# Его можно найти в личном кабинете YooKassa в разделе "Настройки" -> "Магазин".
YOOKASSA_SHOP_ID=1234567

# Секретный ключ для работы с API YooKassa.
# Его можно найти в личном кабинете YooKassa в разделе "Интеграция" -> "Ключи API".
# Для тестирования используйте тестовый ключ (начинается с `test_`),
# для приема реальных платежей - боевой ключ (начинается с `live_`).
YOOKASSA_SECRET_KEY=test_xjh123sdf...

# Перед тем как ставить true, убедитесь, что вы настроили все необходимые параметры в YooKassa.
# Если вы хотите использовать СБП (Систему Быстрых Платежей),
# убедитесь, что у вас включена поддержка СБП в настройках магазина YooKassa.
# Если вы не планируете использовать СБП, оставьте false или поле пустым.
SBP_ENABLED=true  